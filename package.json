{
  "name": "workspace-notes",
  "displayName": "workspace-notes",
  "description": "Manage markdown notes in a centralized repository, while access them quickly based on workspace.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:workspace-notes.newNote"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "workspace-notes.newNote",
        "title": "Workspace Notes: Create a New Note"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "from workspace-note",
      "properties": {
        "workspaceNotes.defaultNotePath": {
          "type": "string",
          "default": "",
          "description": "Path to note repository."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "fs-extra": "^10.0.0"
  }
}
